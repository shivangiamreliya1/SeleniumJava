package Abstraction;

abstract class M1 {

	public void m1() {

		System.out.println("Hello1");
	}

	public void m2(int a, int b) {

		System.out.println("Hello2");
	}

}

class AB1 extends M1 {
	
	int a = 1;
	int b = 2;

	public static void main(String[] args) {
		
		AB1 ab = new AB1();
		ab.m1();
		
		ab.m2(a, b);
		
	}

}
